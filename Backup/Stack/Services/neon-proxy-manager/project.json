{
  "version": "1.0.0-*",

  "buildOptions": {
    "emitEntryPoint": true,
    "xmlDoc": true,
    "compile": {
      "include": [
        "../../Lib/Neon.Stack.Global.Shared/**/*.cs"
      ]
    }
  },

  "dependencies": {
    "Consul": "0.7.2.1",
    "Neon.Cluster.NetCore": "1.0.0-*",
    "Neon.Stack.Common.NetCore": "1.0.0-*",
    "Newtonsoft.Json": "9.0.1",
    "SharpZipLib.Portable": "0.86.0.3",
    "System.Diagnostics.Contracts": "4.3.0",
    "System.Diagnostics.Process": "4.3.0",
    "System.Runtime.Loader": "4.3.0",
    "System.Security.Cryptography.Algorithms": "4.3.0",
    "System.Security.Cryptography.Primitives": "4.3.0",
    "System.Threading.Thread": "4.3.0"
  },

  "frameworks": {
    "netcoreapp1.1": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "type": "platform",
          "version": "1.1.0"
        }
      },
      "imports": "net45"
    }
  },

  "runtimes": {
    "win10-x64": {},
    "ubuntu.16.04-x64": {}
  },

  "runtimeOptions": {
    "configProperties": {
      "System.GC.Server": true
    }
  }
}
